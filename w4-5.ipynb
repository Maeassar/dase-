   "source": [
    "import math\n",
    "import random\n",
    "c = 0\n",
    "N = 10000000\n",
    "\n",
    "def f(a):\n",
    "    return math.pow(a,3)+math.pow(a,2)\n",
    "\n",
    "for i in range(N):\n",
    "    x = random.uniform(0.0,1.0)\n",
    "    y = random.uniform(0.0, 10)\n",
    "    if y <= f(x):\n",
    "        c = c + 1\n",
    "print(c / N * 10)\n"
   ]
